{
  "version":2,
  "builds":[
    {
      "src":"./wsgi.py",
      "use":"@vercel/python",
      "config":{
        "runtime":"python3.11",
        "buildCommand": "gunicorn -w 2 -k uvicorn.workers.UvicornWorker wsgi:app",
        "installCommand": "pip install -r requirements.txt"
      }
    }
  ],
  "routes":[
    {
      "src":"/(.*)",
      "dest":"/"
    }
  ]
}
